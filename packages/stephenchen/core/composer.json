{
    "name": "stephenchen/core",
    "description": "Core packages",
    "keywords": [
        "stephenchen",
        "core"
    ],
    "homepage": "https://github.com/stephenchen/core",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Stephen chen",
            "email": "tasb00429@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^8.0",
        "spatie/laravel-permission": "^5.4",
        "darkaonline/l5-swagger": "^8.0",
        "laravel/socialite": "^5.0",
        "tymon/jwt-auth": "^1.0",
        "prettus/l5-repository": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Stephenchen\\Core\\": "src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stephenchen\\Core\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stephenchen\\Core\\CoreServiceProvider"
            ],
            "aliases": {
                "Core": "Stephenchen\\Core\\CoreFacade"
            }
        }
    }
}
